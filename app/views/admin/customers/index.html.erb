<div class="col-md-8 offset-md-1">
      <h4>会員一覧</h4>
      <table class="table table-hover table-inverse">
        <thead>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @customers.each do |customer| %>
            <tr>
              <td>
                <!--会員ID-->

                <%= customer.id %>
              </td>
              <td>
                <!--氏名-->
                <%= link_to customer.name, admin_item_path(item.id) %>
              </td>
              <td>
                <!--メールアドレス-->
                <%= customer.email %>
              </td>
              <td>
                <!--ステータス-->
                <%= customer.is_active %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>

